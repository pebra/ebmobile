<?xml version='1.0' encoding='utf-8'?>
<widget id="de.pludoni.empfehlungsbundmobile" version="2.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <permission description="Required for accessing user location." name="geolocation" />
    <feature name="Device">
        <param name="firefoxos-package" value="Device" />
    </feature>
    <name>Empfehlungsbund</name>
    <description>
    Stellen des Empfehlungsbundes.
  </description>
    <author email="app@pludoni.de" href="http://www.empfehlungsbund.de">pludoni GmbH</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <cdv:engine id="android" />
    <cdv:engine id="firefoxos" />
    <cdv:engine id="ios" />
</widget>
