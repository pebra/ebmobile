<?xml version='1.0' encoding='utf-8'?>
<widget id="de.pludoni.empfehlungsbundmobile" version="2.1.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="UniversalAnalytics">
        <param name="android-package" value="com.danielcwilson.plugins.analytics.UniversalAnalyticsPlugin" />
    </feature>
    <feature name="Diagnostic">
        <param name="android-package" value="com.neton.cordova.diagnostic.Diagnostic" />
    </feature>
    <feature name="PushPlugin">
        <param name="android-package" value="com.plugin.gcm.PushPlugin" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Toast">
        <param name="android-package" value="nl.xservices.plugins.Toast" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="SocialMessage">
        <param name="android-package" value="uk.co.ilee.socialmessage.SocialMessage" />
    </feature>
    <icon density="mdpi" src="res/icon-mdpi.png" />
    <icon density="hdpi" src="res/icon-hdpi.png" />
    <icon density="xhdpi" src="res/icon-xhdpi.png" />
    <icon density="xxhdpi" src="res/icon-xxhdpi.png" />
    <icon density="xxxhdpi" src="res/icon-xxxhdpi.png" />
    <name>Empfehlungsbund</name>
    <description>
    Stellen des Empfehlungsbundes.
  </description>
    <author email="app@pludoni.de" href="http://www.empfehlungsbund.de">pludoni GmbH</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="permissions" value="none" />
    <cdv:engine id="android" />
    <cdv:engine id="firefoxos" />
    <cdv:engine id="ios" />
</widget>
